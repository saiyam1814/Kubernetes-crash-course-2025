{
  "mcpServers": {
    "redis": {
      "command": "uvx",
      "args": [
        "mcp-server-redis"
      ],
      "env": {
        "REDIS_HOST": "localhost",
        "REDIS_PORT": "6379",
        "REDIS_DB": "0",
        "REDIS_PASSWORD": "mypassword"
      },
      "disabled": false,
      "autoApprove": [
        "set_value"
      ]
    }
  }
}